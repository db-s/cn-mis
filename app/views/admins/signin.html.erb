<h1>Sign In</h1>
<%= form_for(:admin, :url => {:controller => 'admins', :action => 'login'}) do |f| %>
    <p> Email:</br> <%= f.text_field :email%> </p>
    <p> Password:</br> <%= f.password_field :password%></p>
    <%= f.submit 'Sign In' %>
    <%= f.submit 'Clear Form', :type => 'reset' %>
<% end %>


